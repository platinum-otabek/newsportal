doctype html
html.no-js
    head
        title= title
        meta(charset='utf-8')
        meta(http-equiv='x-ua-compatible', content='ie=edge')
        meta(name='description', content='')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        link(rel='manifest', href='site.webmanifest')
        link(rel='shortcut icon', type='image/x-icon', href='/img/favicon.ico')
        link(rel='stylesheet', href='https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css', integrity='sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk', crossorigin='anonymous')
        link(rel='stylesheet', href='/css/owl.carousel.min.css')
        link(rel='stylesheet', href='/css/ticker-style.css')
        link(rel='stylesheet', href='/css/flaticon.css')
        link(rel='stylesheet', href='/css/slicknav.css')
        link(rel='stylesheet', href='/css/animate.min.css')
        link(rel='stylesheet', href='/css/magnific-popup.css')
        link(rel='stylesheet', href='/css/fontawesome-all.min.css')
        link(rel='stylesheet', href='/css/themify-icons.css')
        link(rel='stylesheet', href='/css/slick.css')
        link(rel='stylesheet', href='/css/nice-select.css')
        link(rel='stylesheet', href='/css/style.css')

    body
        #preloader-active
            .preloader.d-flex.align-items-center.justify-content-center
                .preloader-inner.position-relative
                    .preloader-circle
                    .preloader-img.pere-text
                        img(src='/img/logo/logo.png', alt='')
        header
            // Header Start
            .header-area
                .main-header
                    //
                      <div class="header-top black-bg d-none d-sm-block">
                      <div class="container">
                      <div class="col-xl-12">
                      <div class="row d-flex justify-content-between align-items-center">
                      <div class="header-info-left">
                      <ul>
                      <li class="title"><span class="flaticon-energy"></span> trending-title</li>
                      <li>Class property employ ancho red multi level mansion</li>
                      </ul>
                      </div>
                      <div class="header-info-right">
                      <ul class="header-date">
                      <li><span class="flaticon-calendar"></span> +880166 253 232</li>
                      </ul>
                      </div>
                      </div>
                      </div>
                      </div>
                      </div>
                    .header-mid.gray-bg
                        .container
                            .row.d-flex.align-items-center
                                // Logo
                                .col-xl-3.col-lg-3.col-md-3.d-none.d-md-block
                                    .logo
                                        a(href='index.html')
                                            img(src='assets/img/logo/logo.png', alt='')
                                .col-xl-9.col-lg-9.col-md-9
                                    .header-banner.f-right
                                        img(src='assets/img/gallery/header_card.png', alt='')
                    .header-bottom.header-sticky
                        .container
                            .row.align-items-center
                                .col-xl-8.col-lg-8.col-md-12.header-flex
                                    // sticky
                                    .sticky-logo
                                        a(href='index.html')
                                            img(src='assets/img/logo/logo.png', alt='')
                                    // Main-menu
                                    .main-menu.d-none.d-md-block
                                        nav
                                            ul#navigation
                                                li
                                                    a(href='index.html') Home
                                                li
                                                    a(href='about.html') about
                                                li
                                                    a(href='categori.html') Category
                                                li
                                                    a(href='latest_news.html') Latest News
                                                li
                                                    a(href='#') Pages
                                                    ul.submenu
                                                        li
                                                            a(href='blog.html') Blog
                                                        li
                                                            a(href='blog_details.html') Blog Details
                                                        li
                                                            a(href='elements.html') Element
                                                li
                                                    a(href='contact.html') Contact
                                .col-xl-4.col-lg-4.col-md-4
                                    .header-right.f-right.d-none.d-lg-block
                                        // Heder social
                                        ul.header-social
                                            li
                                                a(href='https://www.fb.com/sai4ull')
                                                    i.fab.fa-facebook-f
                                            li
                                                a(href='#')
                                                    i.fab.fa-twitter
                                            li
                                                a(href='#')
                                                    i.fab.fa-instagram
                                            li
                                                a(href='#')
                                                    i.fab.fa-youtube
                                        // Search Nav
                                        .nav-search.search-switch
                                            i.fa.fa-search
                                // Mobile Menu
                                .col-12
                                    .mobile_menu.d-block.d-md-none
            // Header End

        block content

        script(src='https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js' )
        // Jquery, Popper, Bootstrap
        script(src='https://code.jquery.com/jquery-3.5.1.min.js', integrity='sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=', crossorigin='anonymous')
        script(src='/js/bootstrap.min.js')
        // Jquery Mobile Menu
        script(src='/js/jquery.slicknav.min.js')
        // Jquery Slick , Owl-Carousel Plugins
        script(src='https://cdnjs.cloudflare.com/ajax/libs/owl-carousel/1.3.3/owl.carousel.min.js')
        script(src='/js/slick.min.js')
        // Date Picker
        script(src='/js/gijgo.min.js')
        // One Page, Animated-HeadLin
        script(src='https://cdnjs.cloudflare.com/ajax/libs/wow/1.1.2/wow.min.js')
        script(src='/js/animated.headline.js')
        script(src='https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/jquery.magnific-popup.min.js')
        // Scrollup, nice-select, sticky
        script(src='/js/jquery.scrollUp.min.js')
        script(src='/js/jquery.nice-select.min.js')
        script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery.sticky/1.0.4/jquery.sticky.min.js')
        // contact js
        script(src='/js/contact.js')
        script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery.form/4.3.0/jquery.form.min.js')
